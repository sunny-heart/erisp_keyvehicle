<div class="modal-header" style="height:50px;border-bottom: solid 2px #666666;">
	<button type="button" class="close" data-dismiss="modal" aria-hidden="true" ng-click="cancel()">×</button>
	<h3 class="modal-title" style="color: #000000 !important;font-size:18px;">预警修改</h3>
</div>
<div class="modal-body">
	<div class="row padding-top-10 padding-bottom-10">
		<div class="row margin-left-10">
			<div class="pams_span"></div>
		</div>
		<div class="row margin-left-15">
			 <div class="col-md-4">
				<div class="form-group">
					<label>主车类型 :</label>
					<select id="vehicleTypeMain" class="pams_input" style="width:70%;" ng-model="warningData.vehicleTypeMain" disabled>
						<option value="物流车" selected="selected">物流车</option>
					</select>
				</div>
			</div>
			 <div class="col-md-4" >
				<div class="form-group">
					<label>车辆类型 :</label>
					<select id="vehicleType" class="pams_input"  style="width:70%;" ng-model="warningData.vehicleType" disabled>
						<option value="新能源" selected="selected">新能源</option>
			            <option value="冷链" selected="selected">冷链</option>
						<option value="危化品" selected="selected">危化品</option>
						<option value="普通" selected="selected">普通</option>
					</select>
				</div>
			</div>
		</div>
		<div class="row margin-left-15">
		    <div class="col-md-4">
				<div class="form-group">
					<label>车牌号码 :</label>
					<input id='plateNumber' type="text" class="pams_input" style="width: 70%;" ng-model="warningData.plateNumber" disabled>
				</div>
			</div>
			<div class="col-md-4">
				<div class="form-group">
					<label>预警类型 :</label>
					<select id='warningType' type="text" class="pams_input" style="width: 70%;" ng-model="warningData.warningType" disabled>
					    <option value="超速" selected="selected">超速</option>
						<option value="违规时间" selected="selected">违规时间</option>
						<option value="违规路线" selected="selected">违规路线</option>
						<option value="疲劳驾驶" selected="selected">疲劳驾驶</option>
					</select>
				</div>
			</div>
		</div>
		<div class="row margin-left-15">
		    <div class="col-md-4" ng-if="warningData.warningType=='超速'">
				<div class="form-group">
					<label>设置限速 :</label>
					<input type="text" class="pams_input" style="width:70%;" ng-model="warningData.limitSpeed">
				</div>
			</div>
			<div class="col-md-4" ng-if="warningData.warningType=='违规时间' " ng-click="addDate()">
				<div class="form-group">
					<label>开始日期 :</label>
					<input id='startDate' type="text" class="form_date pams_input" style="width: 70%;" placeholder="开始日期"  ng-model="warningData.startDate" readonly>
				</div>
			</div>
			<div class="col-md-4" ng-if="warningData.warningType=='违规时间' " ng-click="addDate()">
				<div class="form-group">
					<label>结束日期 :</label>
					<input id='endDate' type="text" class="form_date pams_input" style="width: 70%;" placeholder="结束日期"  ng-model="warningData.endDate"  readonly>
				</div>
			</div>
			
		    <div class="col-md-4" ng-if="warningData.warningType=='违规路线' ">
				<div class="form-group">
					<label>设置路线 :</label>
					<input id="route" type="text" class="pams_input" style="width: 70%;" ng-model="warningData.warningRoute">
				</div>
			</div>
			<div class="col-md-4">
				<div class="form-group" ng-if="warningData.warningType=='疲劳驾驶' ">
					<label>驾驶时长 :</label>
					<input id="drivingTime" type="text" class="pams_input" style="width: 70%;" ng-model="warningData.drivingTime">
				</div>
			</div>
		</div>
		<div class="row margin-left-15">
		  <div class="col-md-4" ng-if="warningData.warningType=='违规时间' " ng-click="addtime()">
				<div class="form-group">
					<label>开始时间 :</label>
					<input id='startTime' type="text" class="form_datetime pams_input" style="width: 70%;" placeholder="开始时间" ng-model="warningData.startTime"  readonly>
				</div>
			</div>
		    <div class="col-md-4" ng-if="warningData.warningType=='违规时间' " ng-click="addtime()">
				<div class="form-group">
					<label>结束时间 :</label>
					<input id='endTime' type="text" class="form_datetime pams_input" style="width: 70%;" placeholder="结束时间" ng-model="warningData.endTime" readonly>
				</div>
			</div>
		
		</div>
			<div class="row-fluid clearfix padding-top-30">
				<div class="col-md-6 col-md-offset-3">
					<div class="col-md-4">
						<button type="button" ng-click="update()" class="btn pams_query">
							<span>确定<!-- <i class="fa fa-check"></i> --></span>
						</button>
					</div>
					<div class="col-md-4" >
						<button type="button" ng-click="reset()" class="btn pams_reset">
							<span>重置<!-- <i class="fa fa-repeat"></i> --></span>
						</button>
					</div>
					<div class="col-md-4" >
						<button type="button" ng-click="cancel()" class="btn pams_cancel">
							<span>取消<!-- <i class="fa fa-times"></i> --></span>
						</button>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>