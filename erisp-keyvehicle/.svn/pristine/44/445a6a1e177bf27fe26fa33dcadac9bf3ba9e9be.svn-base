<div class="container-fluid container-fullw bg-white animated zoomInDown" style="padding-top: 0;padding-left: 50px; padding-right: 50px;" id="inn">
	<div ng-controller="warningRuleController">
		<div class="title_text" style="display:inline-block;width:100%;">疲劳预警规则录入
			<button type="button" class="btn pams_query" style="float:right" ng-click="saveTiredRule()">
				<span>保存</span>
			</button>
		</div>
		<div data-ng-init="loadRuleData()" ></div>
		<div class="title_line"></div>
		<div class="row-fluid">
			<table
				class="table table-hover table-bordered table-responsive pams_table">
				<input type="hidden" id="tiredRuleId" ng-model="ruleData.id">
				<input type="hidden" name="ruleType" value="TIRED" ng-init="ruleData.ruleType='TIRED'">
				<tr>
					<td class="center" style="background:#f2f2f2">规则名称</td>
					<td class="left table-td">
						<input type="text" name="ruleName" ng-model="ruleData.name" maxlength="50" style="width: 500px">
					</td>
				</tr>
				<tr>
					<td class="center" style="background:#f2f2f2">车辆类型</td>
					<td class="left table-td">
						<div ng-repeat="s in getVehicleTypes()" style="display: inline-block;">
							<input id="vehicleType{{s.id}}" type="checkbox" name="vehicleType" value="{{s.id}}" ng-checked="isCheck(s.id)">
							<label for="vehicleType{{s.id}}" style="margin-right: 16px;">{{s.name}}</label>
						</div>
					</td>
				</tr>
				<tr id="warningType_tired">
					<td class="center" style="background:#f2f2f2">疲劳范围</td>
					<td class="left table-td">
						<div>连续<input type="number" name="driveHour" style="border: 0;background: none;border-bottom: 1px solid #c8c7cc;" ng-model="ruleData.rule[0].driveHour">小时</div>
						<div>移动率<input type="number" name="moveRate" style="border: 0;background: none;border-bottom: 1px solid #c8c7cc;" ng-model="ruleData.rule[0].moveRate">%</div>
					</td>
				</tr>
			</table>
		</div>
	</div>
</div>