<div
	class="container-fluid container-fullw bg-white animated zoomInDown"
	style="padding-top: 0; padding-left: 50px; padding-right: 50px;"
	id="inn">
	<div ng-controller="warningSignController">
		<div class="title_text">预警签收查询</div>
		<div class="title_line"></div>
		<div class="row-fluid">
			<div class="col-md-4" style="padding-left: 0px;">
				<div class="form-group">
					<label class="font_black_famil">车牌号码 :</label> <input
						id="plateNumber" type="text" class="pams_input font_black_famil"
						style="width: 70%;" ng-model="queryConditionData.plateNumber">
				</div>
			</div>
			<div class="col-md-4" style="padding-left: 0px;">
				<div class="form-group">
					<label class="font_black_famil">签收状态</label> <select
						class="js-data-example-ajax font_black_famil" id="warningProcess"
						style="width: 70%;" ng-model="queryConditionData.warningProcess">
						<option value="" selected="selected">全部</option>
						<option value="未签收">未签收</option>
						<option value="已签收">已签收</option>
					</select>
				</div>
			</div>
			<div class="col-sm-8" style="padding-left: 0px;">
				<button type="button" ng-click="query()" class="btn pams_query">
					<span>查询<!-- <i class="fa fa-repeat"></i> --></span>
				</button>
				<button type="button" ng-click="reset()" class="btn pams_reset"
					style="margin-left: 15px;">
					<span>重置<!-- <i class="fa fa-repeat"></i> --></span>
				</button>
				<button type="button" ng-click="updateSignState()"
					class="btn pams_query" style="margin-left: 15px;">
					<span>签收</span>
				</button>
			</div>
		</div>
		<div class="row-fluid">
			<table
				class="table table-hover table-bordered table-responsive pams_table">
				<thead>
					<tr>
						<th class="center table-th" style="cursor: pointer;"
							ng-click="swapCheck()">全选</th>
						<th class="center table-th">车牌号码</th>
						<th class="center table-th">车牌类别</th>
						<th class="center table-th">预警时间</th>
						<th class="center table-th">预警地点</th>
						<th class="center table-th">预警类型</th>
						<th class="center table-th">签收状态</th>
					</tr>
				</thead>
				<tbody>
					<tr ng-repeat="data in warningSignList">
						<td class="center table-td"><input type="checkbox"
							name="warningId" value="{{data.id}}"></td>
						<td class="center table-td">{{data.plateNumber}}</td>
						<td class="center table-td">{{data.plateType}}</td>
						<td class="center table-td">{{data.warningStartTime.time |
							date:'yyyy-MM-dd HH:mm:ss'}}</td>
						<td class="center table-td">{{data.warningStartLocation}}</td>
						<td class="center table-td">{{data.warningType |
							warningInfo}}</td>
						<td class="center table-td">{{data.signState}}</td>
					</tr>
				</tbody>
			</table>
		</div>
		<div style="text-align: right; margin-bottom: 12px;">
			<div class="center" ng-show="totalItems>0"
				style="display: inline-block; padding-right: 10px;">
				查询结果：共计<span style="font-weight: bold">{{totalItems}}</span>条
				&nbsp;当前页<span style="font-weight: bold">{{currentPage}}</span>/共<span
					style="font-weight: bold">{{pages}}</span>页
			</div>
			<input id="first" class="btn" type="button" value="首页"
				style="height: 28px;" ng-click="query()"> <input
				id="previous" class="btn" type="button" value="上一页"
				style="height: 28px;" ng-click="previousPage()"> <input
				id="next" class="btn" type="button" value="下一页"
				style="height: 28px;" ng-click="nextPage()"> <input
				id="last" class="btn" type="button" value="末页" style="height: 28px;"
				ng-click="lastPage()"> <input id="pageNumber" class=""
				type="text" style="height: 28px; width: 70px;" ng-model="anyPage">
			<input id="go" class="btn" type="button" value="GO"
				style="height: 28px;" ng-click="go()">
		</div>
	</div>
</div>